{
  "name": "pagarme-connect",
  "version": "0.1.4",
  "description": "",
  "main": "dist/pagarme.js",
  "scripts": {
    "start": "webpack --progress --colors --optimize-minimize --optimize-dedupe",
    "docs": "jsdoc src -r --verbose --debug -d docs",
    "test": "run-s test:unit",
    "pretest": "npm start",
    "test:e2e": "jest ./e2e --verbose",
    "test:unit": "run-p test:unit:server test:unit:tests",
    "test:unit:server": "env PORT=8080 babel-node ./test/unit/echoServer",
    "test:unit:tests": "wait-on tcp:127.0.01:8080 && jest ./unit --verbose",
    "posttest:unit:tests": "killall -INT echoServer"
  },
  "keywords": [],
  "author": "Pagar.me Pagamentos S/A",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.17.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.16.0",
    "bluebird": "3.4.6",
    "eslint": "3.7.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "jest": "18.0.0",
    "json-loader": "0.5.4",
    "node-fetch": "1.6.3",
    "npm-run-all": "4.0.2",
    "openport": "^0.0.4",
    "qs": "6.3.0",
    "ramda": "0.22.1",
    "wait-on": "2.0.2",
    "webpack": "1.13.2"
  },
  "jest": {
    "testPathDirs": [
      "test"
    ],
    "testRegex": ".spec.js$"
  }
}
